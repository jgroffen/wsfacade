<?xml version='1.0' encoding='utf-8'?>
<Context privileged="true">
	<WatchedResource>WEB-INF/web.xml</WatchedResource>

	<!-- Connection proxy settings are required if the wsf service needs to use a HTTP proxy to access the service it is configured to interpret. -->
	<Environment name="connection_proxy.enabled" type="java.lang.Boolean" value="False" ></Environment>
	<Environment name="connection_proxy.protocol" type="java.lang.String" value="http" description="Protocl of the connection proxy." ></Environment>
	<Environment name="connection_proxy.url" type="java.lang.String" value="10.1.1.1" description="URL of the connection proxy." ></Environment>
	<Environment name="connection_proxy.port" type="java.lang.Integer" value="80" description="Port of the connection proxy." ></Environment>
	<Environment name="connection_proxy.username" type="java.lang.String" value="username" description="Username expected by the connection proxy if authentication is required." ></Environment>
	<Environment name="connection_proxy.password" type="java.lang.String" value="password" description="Password expected by the connection proxy if authentication is required." ></Environment>


	<Environment name="proxies" type="java.lang.String" value="wmts_local,wmts_landgate,wmts_uab" description="List of proxies." ></Environment>


	<Environment name="proxies.wmts_local.custom_proxy" type="java.lang.String" value="com.lisasoft.wsfacade.proxies.WmtsProxy" ></Environment>
	<Environment name="proxies.wmts_local.url" type="java.lang.String" value="/wmts" description="URL of this proxy" ></Environment>
	<Environment name="proxies.wmts_local.proxy_managed" type="java.lang.String" value="?WSDL" description="Comma separated list of URL's managed by the proxy and not passed on to the translated service." ></Environment>

	<Environment name="proxies.wmts_local.client.type" type="java.lang.String" value="soap" ></Environment>
	<Environment name="proxies.wmts_local.client.custom_mapper" type="java.lang.String" value="com.lisasoft.wsfacade.mappers.wmts.WmtsSoapMapper" ></Environment>

	<Environment name="proxies.wmts_local.server.type" type="java.lang.String" value="rest" ></Environment>
	<Environment name="proxies.wmts_local.server.url" type="java.lang.String" value="http://localhost/tilecache/tilecache.py/" ></Environment>
	<Environment name="proxies.wmts_local.server.custom_mapper" type="java.lang.String" value="com.lisasoft.wsfacade.mappers.wmts.WmtsRestMapper" ></Environment>

	<Environment name="proxies.wmts_local.mappings" type="java.lang.String" value="GetCapabilities,GetTile,GetFeatureInfo" ></Environment>


	<Environment name="proxies.wmts_landgate.proxy_managed" type="java.lang.String" value="?WSDL" description="Comma separated list of URL's managed by the proxy and not passed on to the translated service." ></Environment>
	<Environment name="proxies.wmts_landgate.custom_proxy" type="java.lang.String" value="com.lisasoft.wsfacade.proxies.WmtsProxy" ></Environment>
	<Environment name="proxies.wmts_landgate.url" type="java.lang.String" value="/landgate" description="URL of this proxy" ></Environment>

	<Environment name="proxies.wmts_landgate.client.type" type="java.lang.String" value="soap" ></Environment>
	<Environment name="proxies.wmts_landgate.client.custom_mapper" type="java.lang.String" value="com.lisasoft.wsfacade.mappers.wmts.WmtsSoapMapper" ></Environment>

	<Environment name="proxies.wmts_landgate.server.type" type="java.lang.String" value="rest" ></Environment>
	<Environment name="proxies.wmts_landgate.server.url" type="java.lang.String" value="http://beta.landgate.wa.gov.au/tilecache2/wmts.py/" ></Environment>
	<Environment name="proxies.wmts_landgate.server.custom_mapper" type="java.lang.String" value="com.lisasoft.wsfacade.mappers.wmts.WmtsRestMapper" ></Environment>

	<Environment name="proxies.wmts_landgate.mappings" type="java.lang.String" value="GetCapabilities,GetTile,GetFeatureInfo" ></Environment>


	<Environment name="proxies.wmts_uab.proxy_managed" type="java.lang.String" value="?WSDL" description="Comma separated list of URL's managed by the proxy and not passed on to the translated service." ></Environment>
	<Environment name="proxies.wmts_uab.custom_proxy" type="java.lang.String" value="com.lisasoft.wsfacade.proxies.WmtsProxy" ></Environment>
	<Environment name="proxies.wmts_uab.url" type="java.lang.String" value="/uab" description="URL of this proxy" ></Environment>

	<Environment name="proxies.wmts_uab.client.type" type="java.lang.String" value="soap" ></Environment>
	<Environment name="proxies.wmts_uab.client.custom_mapper" type="java.lang.String" value="com.lisasoft.wsfacade.mappers.wmts.WmtsSoapMapper" ></Environment>

	<Environment name="proxies.wmts_uab.server.type" type="java.lang.String" value="rest" ></Environment>
	<Environment name="proxies.wmts_uab.server.url" type="java.lang.String" value="http://www.opengis.uab.es/SITiled/world/" ></Environment>
	<Environment name="proxies.wmts_uab.server.custom_mapper" type="java.lang.String" value="com.lisasoft.wsfacade.mappers.wmts.WmtsRestMapper" ></Environment>

	<Environment name="proxies.wmts_uab.mappings" type="java.lang.String" value="GetCapabilities,GetTile,GetFeatureInfo" ></Environment>
</Context>